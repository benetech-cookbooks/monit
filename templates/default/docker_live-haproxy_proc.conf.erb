check program docker_live-haproxy_proc with path "/usr/bin/docker top live-haproxy"
    start   program = "/usr/bin/docker start live-haproxy" with timeout 120 seconds
    stop    program = "/usr/bin/docker stop live-haproxy" with timeout 120 seconds
    if status != 0 then restart
    if 10 restarts within 10 cycles then unmonitor