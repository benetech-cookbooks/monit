check program docker_live-pgbouncer_proc with path "/usr/bin/docker top live-pgbouncer"
    start   program = "/usr/bin/docker start live-pgbouncer" with timeout 120 seconds
    stop    program = "/usr/bin/docker stop live-pgbouncer" with timeout 120 seconds
    if status != 0 then restart
    if 10 restarts within 10 cycles then unmonitor