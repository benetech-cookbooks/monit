check program docker_live-tomcat-tts_proc with path "/usr/bin/docker top live-tomcat-tts"
    start   program = "/usr/bin/docker start live-tomcat-tts" with timeout 120 seconds
    stop    program = "/usr/bin/docker stop live-tomcat-tts" with timeout 120 seconds
    if status != 0 then restart
    if 10 restarts within 10 cycles then unmonitor